                        owl:onProperty :display_symbol ] [ a owl:Restriction ;
                        owl:hasValue <https://github.com/LiUSemWeb/units-of-measure#www.bipm.org/en/committees/ci/cipm/41-1946/resolution-2[158c0e1e9b]> ;
                        owl:onProperty :has_resource ] [ a owl:Restriction ;
                        owl:hasValue <https://github.com/LiUSemWeb/units-of-measure#www.bipm.org/en/committees/cg/cgpm/11-1960/resolution-12[6235268733]> ;
                        owl:onProperty :has_resource ] [ a owl:Restriction ;
                        owl:hasValue <https://github.com/LiUSemWeb/units-of-measure#en.wikipedia.org/wiki/Ampere[c780e9aa98]> ;
                        owl:onProperty :has_resource ] [ a owl:Restriction ;
